<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1-0.01+dev" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova-native-demo</name>
    <description>
        A sample Apache Cordova application that starts a native demo when it receives the deviceready event.
    </description>
    <author email="brodybits@litehelpers.net" href="https://litehelpers.net">
        @brodybits (Christopher J. Brody aka Chris Brody)
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-syr-core-experimental" spec="https://github.com/phonegap-native-demo/cordova-plugin-syr-core-experimental" />
</widget>
